package cen4010.pa1;

public class Vector3D {
	
	private double xCoord;
	private double yCoord;
	private double zCoord;

	public Vector3D(double x, double y, double z) {
		double xCoord = x;
		double yCoord = y;
		double zCoord = z;
	}
	
	public double dot(Vector3D v){
		double dotP = v.xCoord  
	}
	
}
